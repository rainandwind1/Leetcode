    "class Solution(object):\n",
    "    def waysToChange(self, n):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        dp = [1] + [0]*n\n",
    "        for c in [1, 5, 10, 25]:\n",
    "            for i in range(c, n+1):\n",
    "                dp[i] += dp[i-c]\n",
    "        return dp[-1]%(10**9+7)\n"
   ]
  }
