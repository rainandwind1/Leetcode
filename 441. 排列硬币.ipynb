    "class Solution(object):\n",
    "    def arrangeCoins(self, n):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        index = 1\n",
    "        while n >= index:\n",
    "            n = n - index\n",
    "            index += 1\n",
    "        return index - 1"
   ]
  }
