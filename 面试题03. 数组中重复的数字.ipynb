    "class Solution(object):\n",
    "    def findRepeatNumber(self, nums):\n",
    "        \"\"\"\n",
    "        :type nums: List[int]\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        a = {}\n",
    "        for i in nums:\n",
    "            a[i] = a.get(i,0) + 1\n",
    "            if a[i] > 1:\n",
    "                return i"
